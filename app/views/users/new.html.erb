<div class="page-header">
  <h1>新規ユーザー登録</h1>
</div>

<div class="panel panel-default">
  <div class="panel-body">
    <%= form_with model: @user, id: "user-form", local: true do |f| %>
      <%= render "fields", f: f %>
      <%= f.submit "登録", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>

<%= javascript_pack_tag "users/form" %>
